#ifndef ARRAYEMPLOYEES_H_
#define ARRAYEMPLOYEES_H_

typedef struct{
 int id;
 char name[51];
 char lastName[51];
 float salary;
 int sector;
 int isEmpty;
}eGenerico;

int iniciarEarray(eGenerico *plistEmployee,int len);
int imprimirEarray(eGenerico *plistEmployee,int len);
int agregarEarray(eGenerico *plistEmployee,int len,int id,char name[],char lastName[],float salary,int sector);
int buscarUnIdArray(eGenerico *plistEmployee,int len,int id);
int eliminarLogIdArray(eGenerico *plistEmployee,int len,int id);
int ordenarArray(eGenerico *plistEmployee,int len,int order);

int modificarUnDatodelArray(eGenerico *plistEmployee, int len);
int operateSalaryEmployees(eGenerico *plistEmployee,int len,float *totalSalary, float *promSalary, int *overProm);
int largoArray(eGenerico *plistEmployee,int len);
int buscarLugarArray(eGenerico *plistEmployee,int len);

#endif /* ARRAYEMPLOYEES_H_ */
